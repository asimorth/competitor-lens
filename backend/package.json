{
  "name": "competitor-lens-backend",
  "version": "1.0.0",
  "description": "CompetitorLens Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "build": "npx prisma generate && tsc",
    "start": "npx prisma db push --accept-data-loss && node start-railway.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "worker": "tsx src/workers/fileProcessor.ts",
    "workers:v2": "tsx src/workers/index.ts",
    "scanner": "tsx src/workers/screenshotScanner.ts",
    "screenshots:scan": "tsx src/scripts/autoScanScreenshots.ts",
    "screenshots:import": "npm run screenshots:scan",
    "screenshots:validate": "tsx src/scripts/validateScreenshotData.ts",
    "import:excel": "tsx src/scripts/importMatrixFromExcel.ts",
    "import:matrix": "tsx src/scripts/importMatrixFromExcel.ts",
    "import:screenshots:smart": "tsx src/scripts/importAllScreenshots.ts",
    "sync:screenshots-to-matrix": "tsx src/scripts/syncScreenshotsToMatrix.ts",
    "sync:screenshots-to-matrix:dry": "tsx src/scripts/syncScreenshotsToMatrix.ts --dry-run",
    "sync:local-files": "tsx src/scripts/syncLocalFiles.ts",
    "sync:local-files:dry": "tsx src/scripts/syncLocalFiles.ts --dry-run",
    "sync:smart": "tsx src/scripts/runSmartSync.ts",
    "sync:smart:dry": "tsx src/scripts/runSmartSync.ts --dry-run",
    "migrate:s3": "tsx src/scripts/migrateToS3.ts",
    "test:s3": "tsx src/scripts/testS3Upload.ts",
    "cleanup:duplicates": "tsx src/scripts/cleanup-duplicates.ts",
    "sync:matrix": "tsx src/scripts/sync-matrix-to-db.ts",
    "sync:screenshots": "tsx src/scripts/sync-screenshots-to-db.ts",
    "sync:all": "npm run cleanup:duplicates && npm run sync:matrix && npm run sync:screenshots",
    "lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@aws-sdk/s3-request-presigner": "^3.922.0",
    "@prisma/client": "^6.16.3",
    "@prisma/extension-accelerate": "^2.0.2",
    "@types/mime-types": "^3.0.1",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.59.0",
    "chokidar": "^3.6.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^3.0.1",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "openai": "^4.104.0",
    "p-queue": "^7.4.1",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.17.2",
    "sharp": "^0.34.4",
    "tesseract.js": "^5.1.1",
    "winston": "^3.18.3",
    "xlsx": "^0.18.5",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.6.1",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^6.16.3",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  }
}